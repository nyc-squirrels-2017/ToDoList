<div>
  <h1>All your lists</h1>
  <a href="/lists/new">Create New List</a>
  <ol>
    <% if @lists %>
      <% @lists.each do |list| %>
        <li>
          <a href="/lists/<%= list.id %>"> <%= list.name.capitalize %> </a>
          <ul>
            <li><a href="/lists/<%= list.id %>/edit">edit</a></li>
            <li><a href="/lists/<%= list.id %>/delete">delete</a></li>
          </ul>
        </li>
      <% end %>
    <% end %>
  </ol>
</div>
