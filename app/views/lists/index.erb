<div class="lists-container">
  <h1>All your lists</h1>
  <a class="new-link" href="/lists/new">Create New List</a>
  <section class="lists-cards">
      <% if @lists %>
        <% @lists.each do |list| %>

          <%= erb :"/lists/_onelist", locals: {list: list} %>

        <% end %>
      <% end %>
  </section>
</div>
